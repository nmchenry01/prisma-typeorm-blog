generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Company {
  createdAt DateTime  @default(now())
  id        Int       @default(autoincrement()) @id
  name      String?
  Product   Product[]
}

model Product {
  createdAt   DateTime @default(now())
  description String?
  id          Int      @default(autoincrement()) @id
  name        String?
  productId   Int
  Company     Company  @relation(fields: [productId], references: [id])
}
